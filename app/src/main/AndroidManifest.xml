<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.wirelessteam.wirelessproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".setupPage"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".Highlights"
            android:parentActivityName=".setupPage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".setupPage" />
        </activity>
        <activity
            android:name=".GameView"
            android:parentActivityName=".setupPage">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".setupPage" />
        </activity>
        <activity
            android:name=".BoardActivity"
            android:parentActivityName=".GameView">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GameView" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login" />
        <activity android:name=".registration"></activity>
    </application>

</manifest>